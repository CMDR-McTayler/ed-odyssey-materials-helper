<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<fx:root fx:id="box"
         type="javafx.scene.layout.HBox"
         xmlns="http://javafx.com/javafx/11" xmlns:fx="http://javafx.com/fxml/1">
    <ImageView fx:id="image" fitHeight="28" fitWidth="28"/>
    <Label fx:id="nameLabel" styleClass="name">test</Label>
    <Label nodeOrientation="RIGHT_TO_LEFT" fx:id="amountRequiredLabel" styleClass="required">test
    </Label>
    <Label nodeOrientation="RIGHT_TO_LEFT" fx:id="amountAvailableLabel" styleClass="available">test
    </Label>
</fx:root>